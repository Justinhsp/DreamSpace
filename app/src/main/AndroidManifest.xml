<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hycf.example.hsp">

    <uses-permission android:name="android.permission.CAMERA"/>


    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.CircleFriendActivity"/>
        <activity android:name=".ui.activity.PublishCircleActivity"/>
        <activity android:name=".ui.activity.WeChatActivity"/>
        <activity android:name=".ui.activity.PostImagesActivity"/>
        <activity android:name=".ui.activity.BelleDetailActivity"/>
        <activity android:name=".ui.activity.NewsDetailNormalActivity"/>
        <activity android:name=".ui.activity.NewsDetailWebViewActivity"/>
        <activity android:name=".ui.activity.NewsDetailPhotoActivity"/>
        <activity android:name=".ui.activity.AboutActivity"/>
        <activity android:name=".ui.activity.NewsChannelActivity"/>
        <activity android:name=".ui.activity.WelComeActivity" />
    </application>

</manifest>